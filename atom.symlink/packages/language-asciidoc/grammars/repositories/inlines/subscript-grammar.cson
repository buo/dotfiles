key: 'subscript'

patterns: [
  # Matches subscript
  #
  # Examples
  #
  #   ~subscript~
  #
  name: 'markup.subscript.asciidoc'
  match: '(?<!\\\\)(\\[.+?\\])?((~)(\\S+?)(~))'
  captures:
    1: name: 'markup.meta.sub.attribute-list.asciidoc'
    2: name: 'markup.sub.subscript.asciidoc'
    3: name: 'punctuation.definition.asciidoc'
    5: name: 'punctuation.definition.asciidoc'
]

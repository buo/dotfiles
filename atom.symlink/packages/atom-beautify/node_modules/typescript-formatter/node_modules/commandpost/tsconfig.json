{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "experimentalDecorators": true,
        "rootDir": ".",
        "sourceMap": true,
        "declaration": true,
        "listFiles": true,
        "newLine": "LF",
        "noEmitOnError": true,
        "inlineSources": true
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./**/*.d.ts",
        "./typings/**/*.d.ts",
        "!./example/**/*.ts",
        "!./node_modules/**/*",
        "./node_modules/typescript/lib/lib.es6.d.ts"
    ],
    "exclude": [],
    "files": [
        "./lib/argument.ts",
        "./lib/command.ts",
        "./lib/index.ts",
        "./lib/option.ts",
        "./lib/utils.ts",
        "./test/argumentSpec.ts",
        "./test/commandSpec.ts",
        "./test/indexSpec.ts",
        "./test/optionSpec.ts",
        "./test/utilsSpec.ts",
        "./typings/empower/empower.d.ts",
        "./typings/mocha/mocha.d.ts",
        "./typings/node/node.d.ts",
        "./typings/power-assert-formatter/power-assert-formatter.d.ts",
        "./typings/power-assert/power-assert.d.ts",
        "./node_modules/typescript/lib/lib.es6.d.ts"
    ]
}